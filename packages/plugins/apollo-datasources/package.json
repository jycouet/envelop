    "graphql": "16.3.0",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@envelop/core": "^2.3.3",
    "apollo-datasource": "^3",
    "apollo-server-caching": "^3",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
